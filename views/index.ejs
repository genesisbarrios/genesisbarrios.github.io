	<script type="module">
		import * as THREE from 'https://threejsfundamentals.org/threejs/resources/threejs/r115/build/three.module.js';
		window.onload = function() {

			// Canvas
			const canvas = document.querySelector('canvas.webgl');

			let scene, camera, renderer, particlesThree, star, stars, particlesCount;
			let particlesMesh;

			scene = new THREE.Scene();
			camera = new THREE.PerspectiveCamera(60, window.innerWidth / window.innerHeight, 1, 1000);
			camera.position.z = 1;
			camera.rotation.x = Math.PI/2;

			renderer = new THREE.WebGLRenderer({  
				canvas: canvas,
				alpha: true 
			});
			renderer.setSize(window.innerWidth, window.innerHeight);
			renderer.setPixelRatio(Math.min(window.devicePixelRatio, 2))
			//canvas.appendChild(renderer.domElement);

			const material = new THREE.MeshBasicMaterial()
			material.color = new THREE.Color(0xffffff)

			const particlesGeo = new THREE.BufferGeometry();
			particlesCount = 10000;
			const posArray = new Float32Array(particlesCount * 3);

			for(let i=0; i < particlesCount * 3; i++){
				posArray[i] = (Math.random() - 0.5) * 5;
			}

			particlesGeo.setAttribute('position', new THREE.BufferAttribute(posArray, 3))

			particlesMesh = new THREE.Points(particlesGeo, material)
			scene.add(particlesMesh);

			document.addEventListener('mouseover', animateParticles);
			let mouseX = 0;
			let mouseY = 0;

			function animateParticles(event){
				console.log('animate particles')
				mouseY = event.clientY;
				mouseX = event.clientX;
			}

			const clock = new THREE.Clock();

			const tick = () => {
				const elapsedTime = clock.getElapsedTime()

				particlesMesh.rotation.y = -.1 * elapsedTime;
				if(mouseX > 0){
					particlesMesh.rotation.y = mouseX * (elapsedTime * 0.0008) 
					particlesMesh.rotation.x = mouseY * (elapsedTime * 0.0008) 
				}

				renderer.render(scene, camera);
				window.requestAnimationFrame(tick);
			}

			tick();

			var i =0;
			var aboutText = "genesisbarrios:~$ cd genesisbarrios/About\r\ngenesisbarrios:~$ cat about.txt \r\n";
			var aboutText2 = "\r\nHello! My name is Genesis Barrios.\r\nI'm a Software Engineer based in Miami, Florida. ðŸŒ´ \r\nI got my Bachelor's of Science in Computer Science in Spring of 2020 from Florida International University.\r\nSome of my topics of study include: C Programming, Java Programming, Data Structures, Discrete Structures,\r\nComputer Architecture, Database Management, Net Centric Computing, Software Engineering,\r\nMobile Application Development, and Programming Languages. \r\nI am currently working as an IT Resident at Assurant where I build and support applications in the Azure cloud.\r\nI have experience with various technologies including Javascript, JQuery, Bootstrap, HTML, CSS, Angular, React,\r\n Node.js, MongoDB, SQL, MySQL, MicrosoftSQL, Java, Tomcat, C#, .NET, Azure Cloud,\r\nSwift, Web GL(Babylon.js, Three.js), Web3.js, Solidity, and Python.";
			var speed = 50;

			function typeWriter(){
				document.getElementById("aboutText").setAttribute("style", "white-space: pre;")
				if(i < aboutText.length){
					document.getElementById("aboutText").innerHTML += aboutText.charAt(i);
					i++;
					setTimeout(typeWriter, speed);
				}
				if(i == aboutText.length - 1) {
					document.getElementById("aboutText2").innerHTML += aboutText2;
				}
			}

			typeWriter();
		};
	</script>
