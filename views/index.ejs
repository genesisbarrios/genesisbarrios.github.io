	<script type="module">
		import * as one from 'https://onejsfundamentals.org/onejs/resources/onejs/r115/build/one.module.js';
		
		

		window.onload = function() {
			var one = new one({
				fullscreen: true,
				autostart: false
			  })
			  
			// Canvas
			const canvas = document.querySelector('canvas.webgl');

			let scene, camera, renderer, particlesone, star, stars, particlesCount;
			let particlesMesh;

			scene = new one.scene();
			camera = new one.PerspectiveCamera(60, window.innerWidth / window.innerHeight, 1, 1000);
			camera.position.z = 1;
			camera.rotation.x = Math.PI/2;

			renderer = new one.WebGLRenderer({  
				canvas: canvas,
				alpha: true 
			});
			renderer.setSize(window.innerWidth, window.innerHeight);
			renderer.setPixelRatio(Math.min(window.devicePixelRatio, 2))
			//canvas.appendChild(renderer.domElement);

			const material = new one.MeshBasicMaterial()
			material.color = new one.Color(0xffffff)

			const particlesGeo = new one.BufferGeometry();
			particlesCount = 10000;
			const posArray = new Float32Array(particlesCount * 3);

			for(let i=0; i < particlesCount * 3; i++){
				posArray[i] = (Math.random() - 0.5) * 5;
			}

			particlesGeo.setAttribute('position', new one.BufferAttribute(posArray, 3))

			particlesMesh = new one.Points(particlesGeo, material)
			scene.add(particlesMesh);

			document.addEventListener('mouseover', animateParticles);
			let mouseX = 0;
			let mouseY = 0;

			function animateParticles(event){
				console.log('animate particles')
				mouseY = event.clientY;
				mouseX = event.clientX;
			}

			const clock = new one.Clock();

			const tick = () => {
				const elapsedTime = clock.getElapsedTime()

				particlesMesh.rotation.y = -.1 * elapsedTime;
				if(mouseX > 0){
					particlesMesh.rotation.y = mouseX * (elapsedTime * 0.0005) 
					particlesMesh.rotation.x = mouseY * (elapsedTime * 0.0005) 
				}

				renderer.render(scene, camera);
				window.requestAnimationFrame(tick);
			}

			tick();

			var i =0;
			var aboutText = " cd genesisbarrios/About\r\ngenesisbarrios:~$ cat about.txt \r\n";
			var aboutText2 = "\r\nHello! My name is Genesis Barrios.\r\nI'm a Software Engineer based in Miami, Florida. ðŸŒ´ \r\nI got my Bachelor's of Science in Computer Science in Spring of 2020 from Florida International University.\r\nI am currently working as an Associate Software Engineer where I build and support applications in the Azure cloud.\r\nI'm a Full Stack Engineer having worked with various technologies including Javascript, JQuery, Angular, React,\r\n Node.js, MongoDB, Firebase, SQL, MySQL, MicrosoftSQL, Tomcat, Java, C#, .NET, Swift, Web GL(Babylon.js, one.js), and Python. I'm\r\n currently dedicating my free time to learning and building with Web3 technologies like Soldiity and Web3.js. I love building creative solutions for myself and others, and I offer support through <a href='https://www.enigmasoftwareconsulting.com' style='color:white; font-weight:bold;' target='_blank'>Enigma Software Consulting.</a>";
			var speed = 50;

			function typeWriter(){
				console.log('typewriter');
				document.getElementById("aboutText").setAttribute("style", "white-space: pre;")
				if(i < aboutText.length){
					document.getElementById("aboutText").innerHTML += aboutText.charAt(i);
					i++;
					setTimeout(typeWriter, speed);
				}
				if(i == aboutText.length - 1) {
					document.getElementById("aboutText2").innerHTML += aboutText2;
				}
			}

			typeWriter();
		};
	</script>
